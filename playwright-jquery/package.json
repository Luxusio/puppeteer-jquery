{
  "name": "playwright-jquery",
  "version": "0.2.1",
  "description": "Fluent jquery integration for playwright in Typescript",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rimraf lib src/*.js src/*.map src/*.d.ts test/*.js test/*.map test/*.d.ts",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "prepare": "npm run clean && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UrielCh/puppeteer-jquery.git"
  },
  "keywords": [
    "jquery",
    "playwright"
  ],
  "author": "uriel Chemouni <uchemouni@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UrielCh/puppeteer-jquery/issues"
  },
  "homepage": "https://github.com/UrielCh/puppeteer-jquery#readme",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/jquery": "^3.5.5",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.37",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "playwright": "^1.10.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "files": [
    "lib",
    "data/*.js"
  ]
}
